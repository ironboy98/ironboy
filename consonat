z=input()
if(z=='A' or z=='a'):
  print('it is a Vowel')
elif(z=='E' or z=='e'):
  print('it is a Vowel')
elif(z=='I' or z=='i'):
  print('it is a Vowel')
elif(z=='O' or z=='o'):
  print('it is a Vowel')
elif(z=='U' or z=='u'):
  print('it is a Vowel')
elif(z>='a' and z<='z') or (z>='A' and z<='Z'):
  print('it is a Consonat')
else:
  print('invalid')
